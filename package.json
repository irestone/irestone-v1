{
  "name": "irestone-v1",
  "author": "Khan Bifov @irestone",
  "description": "Personal website",
  "keywords": [
    "nodejs",
    "pug",
    "stylus"
  ],
  "version": "0.1.0",
  "private": true,
  "license": "ISC",
  "main": "app.js",
  "scripts": {
    "start": "node -r esm ./bin/www",
    "dev": "run-p dev:*",
    "dev:server": "cross-env DEBUG=node-pug:* nodemon -r dotenv/config -r esm ./bin/www --ignore public --ignore node_modules",
    "dev:client": "rollup -c -w",
    "build": "rollup -c",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build"
  },
  "dependencies": {
    "@dogstudio/highway": "^2.1.3",
    "@types/mongoose": "^5.5.12",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "gsap": "^2.1.3",
    "http-errors": "~1.6.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.15",
    "mongoose": "^5.6.9",
    "morgan": "~1.9.1",
    "prettier": "^1.16.4",
    "pug": "2.0.0-beta11",
    "stylus": "0.54.5"
  },
  "devDependencies": {
    "@types/gsap": "^1.20.2",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1"
  }
}
